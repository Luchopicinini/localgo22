        <script type="rocketlazyloadscript">
        document.addEventListener('DOMContentLoaded', function() {
            // Obtiene todos los elementos con la clase .woocommerce-Price-amount
            var priceAmounts = document.querySelectorAll('.woocommerce-Price-amount');

            // Itera sobre cada elemento y envuélvelo con un span.notranslate
            priceAmounts.forEach(function(priceAmount) {
                // Crea el nuevo span con la clase notranslate
                var notranslateWrapper = document.createElement('span');
                notranslateWrapper.className = 'notranslate';

                // Mueve el contenido del elemento .woocommerce-Price-amount dentro del nuevo span
                while (priceAmount.firstChild) {
                    notranslateWrapper.appendChild(priceAmount.firstChild);
                }

                // Reemplaza el elemento .woocommerce-Price-amount con el nuevo span
                priceAmount.parentNode.replaceChild(notranslateWrapper, priceAmount);
            });
        });
