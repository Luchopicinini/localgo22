        <script type="rocketlazyloadscript">
jQuery(document).ready(function() {
    let interval;

    function iniciarTemporizador(expiracionUnix, utc) {
        if (interval) {
            clearInterval(interval);
        }

        // Expiración en tiempo UNIX, ya ajustado a la zona horaria del servidor
        const countDownDate = expiracionUnix * 1000; // Convertir a milisegundos

        interval = setInterval(function() {
            const now = new Date().getTime(); // Obtener la hora actual del cliente en milisegundos
            const distance = countDownDate - now; // Calcular el tiempo restante
            
            const minutes = Math.floor((distance % (1000 * 60 * 60)) / (1000 * 60)); // Corregido para horas y minutos
            const seconds = Math.floor((distance % (1000 * 60)) / 1000);

            if (distance >= 0) {
                const tiempoRestante = minutes + 'm ' + seconds + 's';

                jQuery("#ozytrip-timer").html(tiempoRestante + ' &nbsp; <a href="javascript:void(0)" class="reset-timer" title="Agregar Tiempo"><img src="https://turistik.com/wp-content/themes/oceanwp-child-theme-master/assets/img/extra-time-ozy.png" style="width:16px" alt="Agregar Tiempo"></a>');
                jQuery(".popup-timer-countdown, .cart-timer b").html(tiempoRestante);

                if (distance > 0) {
                    jQuery("#ozytrip-timer").fadeIn();
                } else {
                    clearInterval(interval);
                    jQuery("#ozytrip-timer").html("00:00");
                    jQuery("#ozytrip-timer").fadeOut();
                }

                if (minutes <= 4 && minutes > 0 && jQuery('.popup-timer-close').data('close') !== "no") {
                    jQuery(".popup-timer").fadeIn();
                }
            } else {
                clearInterval(interval);
                jQuery("#ozytrip-timer").html("00:00");
                jQuery("#ozytrip-timer").fadeOut();
            }
        }, 1000);
    }

    function verificarEstadoTimer() {
        const randDebug = Math.random();
        jQuery.ajax({
            url: '/wp-json/tos/v1/get-timer/',
            type: 'GET',
            dataType: 'json',
            cache: false,
            success: function(data) {
                if (data && data.expiracion) {
                    jQuery(".cart-ozy-booking-id").html(data.booking_id);
                    iniciarTemporizador(data.expiracion, data.utc); // Pasar el tiempo de expiración y la hora del servidor
                } else {
                    jQuery("#ozytrip-timer").fadeOut();
                }
            },
            error: function(xhr, status, error) {
                console.error("Error al verificar el temporizador: " + error);
            }
        });
    }

    // Iniciar la verificación periódica del temporizador cada 15 segundos
    setInterval(verificarEstadoTimer, 15000);
    // Verificar el estado inmediatamente al cargar la página
    verificarEstadoTimer();

    jQuery("body").on("click", ".reset-timer, .popup-timer-reset", function() {
        jQuery(".reset-timer").html('<img src="/wp-content/themes/oceanwp-child-theme-master/assets/img/preload-2.gif" style="width:16px" alt="Reset Tiempo">');
        jQuery.get("/reset-timer", function(data) {
            jQuery(".popup-timer-preload").fadeIn();
            location.reload();
        });
    });

    jQuery("body").on("click", ".popup-timer-close", function() {
        jQuery(".popup-timer").fadeOut();
        jQuery(this).data("close", "no");
    });
});
    </script>
               
