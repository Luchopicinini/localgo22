        <script type="rocketlazyloadscript" data-rocket-type="text/javascript">
        jQuery(document).ready(function($) {
            // Itera sobre todos los elementos <a> con la clase elementor-icon
            $('a.elementor-icon').each(function() {
                var $this = $(this);
                var label = $this.find('.elementor-icon-title').text().trim(); // Obtiene el texto dentro de .elementor-icon-title

                // Si no hay texto visible, utiliza el valor del atributo href
                if (!label) {
                    var hrefValue = $this.attr('href');
                    // Extraer el nombre del dominio del href
                    var domain = hrefValue.replace(/^(?:https?:\/\/)?(?:www\.)?([^\/]+)/i, "$1");
                    label = domain.trim(); // Usa el nombre del dominio como texto alternativo
                }

                // Agrega el atributo aria-label al elemento <a> con el texto obtenido
                $this.attr({
                    'aria-label': label,
                    'role': 'link'
                });
            });
