		display: inline-block;
		width: 12px;
		height: 12px;
		border-radius: 50%;
		border: 2px solid #ccc;
		margin-right: 10px;
		vertical-align: middle;
	}


	/* CAmbio de  */

	#payment_methods {
		display: flex;
		justify-content: center;
		gap: 20px; /* Espacio entre cada método de pago */
		margin-top: 20px;
	}

	#payment_methods .payment_method {
		display: flex;
		align-items: center; /* Alinear los elementos verticalmente */
		padding: 10px 30px;
		border: 2px solid #ccc; /* Borde inicial */
		border-radius: 5px; /* Esquinas redondeadas */
		cursor: pointer;
		transition: all 0.3s ease;
		height: 100px; /* Ajustar el alto del cuadro */
		position: relative; /* Para permitir la posición del radio button */
	}

	#payment_methods .payment_method:hover {
		border-color: #ff0000; /* Borde rojo al pasar el ratón */
	}

	/* Mostrar los botones de radio y alinearlos dentro del label */
	#payment_methods .payment_method input[type="radio"] {
		appearance: none; /* Elimina el estilo por defecto del navegador */
		-webkit-appearance: none; /* Para navegadores WebKit */
		width: 20px;
		height: 20px;
		border: 2px solid #ccc;
		border-radius: 50%; /* Hacer que el radio sea redondo */
		outline: none;
		cursor: pointer;
		position: absolute;
		top: 50%;
		left: 15px;
		transform: translateY(-50%); /* Centrar verticalmente el radio button */
	}

	/* Estilo para cuando el radio está seleccionado */
	#payment_methods .payment_method input[type="radio"]:checked {
		border-color: #ff0000; /* Cambia el color del borde cuando está seleccionado */
		background-color: #fff;
	}

	#payment_methods .payment_method input[type="radio"]:checked::before {
		content: '';
		display: block;
		width: 12px;
		height: 12px;
		border-radius: 50%;
		background-color: #ff0000; /* Círculo interno cuando está seleccionado */
		position: absolute;
		top: 4px;
		left: 4px;
	}

	/* Estilo de la imagen dentro del label */
	#payment_methods .payment_method label img {
		height: auto;
		display: block;
		margin-left: auto; /* Empujar la imagen hacia la derecha */
	}

	/* Cuando el botón de radio está seleccionado */
	#payment_methods .payment_method input[type="radio"]:checked + label {
		border: none; /* Eliminar el borde interno alrededor de la imagen */
	}

	#payment_methods .payment_method label {
		display: flex;
		align-items: center;
		justify-content: flex-start;
		width: 100%;
		height: 100%;
		padding-left: 30px; /* Asegurar espacio entre el radio button y el logo */
	}

	/* Alinear la imagen a la derecha */
	#payment_methods .payment_method label img {
		margin-left: 10px; /* Ajustar margen entre el botón de radio y el logo */
	}
	
	.grupos-pagar-abono {
    display: flex;
    width: 435px;
    margin: 10px auto;
    gap: 10px;
    margin-bottom: 25px;
}

@media only screen and (min-width: 769px) {
    .form-paso {
        padding: 0px 40px 6px 40px !important;
        margin: 0px 0px 0 0px !important; 
    }
}
@media screen and (max-width:768px){
  #currency_selector {
    display: grid;
    grid-template-columns: 1fr 1fr;
  }

